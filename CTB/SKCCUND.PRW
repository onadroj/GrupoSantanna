#include "rwmake.ch"

User Function SKCCUND()
LOCAL MCONTAB
MCONTAB:=GETMV("MV_MCONTAB")
_RETORNO:="ERRO NA BUSCA"
IF PARAMIXB<>""
	IF MCONTAB=="CON"
		_RETORNO:=RETFIELD("SI3",1,XFILIAL("SI3")+PARAMIXB,"I3_CCUNID")
	ELSE
		_RETORNO:=RETFIELD("CTT",1,XFILIAL("CTT")+PARAMIXB,"CTT_CCUNID")
	ENDIF
ENDIF
Return(_RETORNO)
